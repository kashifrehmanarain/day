{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "predis/predis": "^1.0",
        "jenssegers/date": "^3.0",
        "barryvdh/laravel-ide-helper": "~2.0",
        "pda/pheanstalk": "~3.0",
        "jenssegers/agent": "~2.1",
        "yozaz/laravel-swiftmailer": "~3.0",
        "styde/blade-pagination": "5.1.*@dev",
        "anhskohbo/no-captcha": "2.*",
        "intervention/image": "~2.1",
        "laracasts/testdummy": "^2.3",
        "barryvdh/laravel-debugbar": "^2.0",
        "gaaarfild/laravel-conf": "~1.0",
        "gaaarfild/laravel-notifications": "~1.0",
        "gaaarfild/laravel-pinger": "~1.0",
        "gaaarfild/laravel-title": "1.*",
        "cviebrock/eloquent-sluggable": "^3.0",
        "symfony/var-dumper": "^4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "phpspec/phpspec": "~2.1",
        "graham-campbell/exceptions": "^8.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "chmod -R 777 public/"        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dev-master"
    }
}
